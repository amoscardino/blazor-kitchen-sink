@page "/javascript"

<h1>JS Interop</h1>

<form class="form-inline" @onsubmit="ShowMessageAsync" @onsubmit:preventDefault>
    <input type="text" @bind="Message" class="form-control mb-2 mr-sm-2" placeholder="Message to send" />

    <button type="submit" class="btn btn-primary mb-2">
        Send
    </button>
</form>
